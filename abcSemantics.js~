var symbolTable;

var interpreter = {
    Assignment_simple: function (v, keq, n) {
	symbolTable[v] = n;
    },

    Assignment_complex: function (v, keq, expr) {
	symbolTable[v] = expr.;
    },

    Expression: function (v1, kplus, v2) {
    },

    Variable: function (c) {
	return c.toString ();
    },

    number: function (ds) {
	return parseFloat (this.sourceString);
    }    
};

