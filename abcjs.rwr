% rewrite ABCjs {
TopLevel [exprs] = ‛«exprs»’

Statement_assignment [v _eq e rec?] = ‛\nlet «v» = «e»;«rec»’
Statement_expr [e rec?] = ‛«e»«rec»’

Expression_plus [left _plus right] = ‛«left» + «right»’
Expression_atom [a] = ‛«a»’

atom [a] = ‛«a»’

variable [c] = ‛«c»’
number [digits+] = ‛«digits»’
dig [c] = ‛«c»’
}
